{
  "name": "@quilicicf/asciidoc-2-reveal",
  "version": "1.0.1",
  "description": "Highly opinionated Reveal.js presentation compiler",
  "bin": {
    "a2r": "cli/a2r.mjs"
  },
  "scripts": {
    "build": "node cli/a2r.mjs build --input-file test/deck.adoc --output-file dist/deck.html",
    "start": "node cli/a2r.mjs watch --input-file test/deck.adoc --output-file dist/deck.html",
    "bundle-deck-base": "node scripts/bundleDeckBase.mjs",
    "update-doc": "node scripts/documentParameters.mjs",
    "update-diagram-styles": "node scripts/writeDiagramStyles.mjs"
  },
  "keywords": [
    "asciidoc",
    "reveal.js",
    "html",
    "presentation",
    "deck",
    "single-file"
  ],
  "author": "quilicicf <quilicicf@gmail.com> (https://github.com/quilicicf)",
  "license": "Apache-2.0",
  "dependencies": {
    "@asciidoctor/core": "3.0.3",
    "@mermaid-js/mermaid-cli": "10.7.0",
    "@minify-html/node": "0.11.1",
    "chroma-js": "2.4.2",
    "jsdom": "24.0.0",
    "json5": "2.2.3",
    "mermaid": "10.8.0",
    "prism-themes": "1.9.0",
    "prismjs": "1.29.0",
    "reveal.js": "5.0.4",
    "sass": "1.70.0",
    "stoyle": "1.0.0",
    "uuid": "9.0.1",
    "ws": "8.16.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "chokidar": "3.5.3",
    "parcel": "2.11.0"
  }
}
